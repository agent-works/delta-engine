{
  "name": "delta-engine",
  "version": "1.3.0",
  "description": "A minimalist platform for accelerating AI Agent prototype iteration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "delta": "./dist/index.js",
    "delta-engine": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules' jest --no-coverage tests/unit",
    "test:integration": "tsx tests/run-integration.ts",
    "test:watch": "jest --watch tests/unit",
    "test:stateless": "tsx tests/integration/stateless-core.test.ts",
    "test:hooks": "tsx tests/integration/hooks.test.ts",
    "test:io": "tsx tests/integration/io-audit.test.ts",
    "clean": "rm -rf dist",
    "start": "node dist/index.js"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "orchestration",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "dotenv": "^17.2.2",
    "execa": "^8.0.1",
    "openai": "^4.28.0",
    "uuid": "^9.0.1",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
