{
  "name": "delta-engine",
  "version": "1.10.0",
  "description": "A minimalist platform for accelerating AI Agent prototype iteration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "delta": "./dist/index.js",
    "delta-engine": "./dist/index.js",
    "delta-sessions": "./dist/sessions-cli.js",
    "delta-sessions-pty": "./dist/sessions-pty-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "test": "npm run test:unit",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules' jest --no-coverage tests/unit",
    "test:e2e": "tsx tests/run-e2e.ts",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:pre-release": "npm run build && npm run test:all",
    "test:watch": "jest --watch tests/unit",
    "clean": "rm -rf dist",
    "start": "node dist/index.js"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "orchestration",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agent-works/delta-engine.git"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "dotenv": "^17.2.2",
    "execa": "^8.0.1",
    "node-pty": "^1.0.0",
    "openai": "^4.28.0",
    "shell-quote": "^1.8.3",
    "uuid": "^9.0.1",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.19",
    "@types/shell-quote": "^1.7.5",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
